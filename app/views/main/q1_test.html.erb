<div class="hero-unit">
  <%= form_tag('p_data', id: 'vform') do -%>
    <%= label_tag 'cs', '测试次数' %>
    <%= select_tag 'cs', options_from_collection_for_select(@cs, :first, :last), prompt: '请选择你要测试的次数', required: true %>
    <br>
    <%= submit_tag('提交数据', class: 'btn btn-primary btn-large') %>
  <% end %>
</div>
